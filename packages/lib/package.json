{
  "name": "mithril-leaflet",
  "version": "0.3.0",
  "description": "A Leaflet drawable map component for mithril.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/erikvullings/mithril-leaflet.git"
  },
  "scripts": {
    "clean": "rimraf .rpt2_cache dist",
    "link": "npm link",
    "typedoc": "typedoc --out ../../docs/typedoc src",
    "build:domain": "npm run build && npm run typedoc",
    "start": "webpack --config scripts/webpack.dev.js",
    "build": "webpack --optimize-minimize --config scripts/webpack.prod.js",
    "dry-run": "npm publish --dry-run",
    "patch-release": "npm version patch --force -m \"Patch release\" && npm run publish",
    "minor-release": "npm version minor --force -m \"Minor release\" && npm run publish",
    "major-release": "npm version major --force -m \"Major release\" && npm run publish",
    "publish": "git commit package.json -m \"version bump\" && npm publish && git push --follow-tags"
  },
  "keywords": [
    "mithril",
    "leaflet",
    "leaflet-draw"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "peerDependencies": {
    "mithril": "^2.0.4",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/leaflet": "^1.5.12",
    "@types/leaflet-draw": "^1.0.2",
    "@types/mithril": "^2.0.3",
    "autoprefixer": "^9.8.0",
    "cssnano": "^4.1.10",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^5.0.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}