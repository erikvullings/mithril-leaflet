{
  "name": "mithril-leaflet",
  "version": "0.1.0",
  "description": "A materialize library for mithril.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/erikvullings/mithril-leaflet.git"
  },
  "scripts": {
    "link": "npm link",
    "styles:watch": "cpx \"src/**/*.png\" \"dist\" --watch",
    "build:old": "tsc -w",
    "build:domain": "typedoc --out ../../docs/typedoc src",
    "start:old": "run-p styles:watch build",
    "start": "webpack --config scripts/webpack.dev.js",
    "build": "webpack --optimize-minimize --config scripts/webpack.prod.js",
    "patch-release": "npm version patch --force -m \"Patch release\" && npm publish && git push --follow-tags",
    "minor-release": "npm version minor --force -m \"Minor release\" && npm publish && git push --follow-tags",
    "major-release": "npm version major --force -m \"Major release\" && npm publish && git push --follow-tags"
  },
  "keywords": [
    "mithril",
    "leaflet"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/leaflet": "^1.4.3",
    "@types/leaflet-draw": "^0.4.13",
    "@types/mithril": "^1.1.16",
    "autoprefixer": "^9.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "cpx": "^1.5.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "style-loader": "^0.23.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
